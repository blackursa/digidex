{
  "name": "digidex",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "node --trace-warnings --unhandled-rejections=strict node_modules/jest/bin/jest.js",
    "test:watch": "node --trace-warnings --unhandled-rejections=strict node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --trace-warnings --unhandled-rejections=strict node_modules/jest/bin/jest.js --coverage",
    "test:accessibility": "node --trace-warnings --unhandled-rejections=strict node_modules/jest/bin/jest.js --testMatch='**/__tests__/accessibility/**/*.test.[jt]s?(x)'",
    "test:performance": "node --trace-warnings --unhandled-rejections=strict node_modules/jest/bin/jest.js --testMatch='**/__tests__/performance/**/*.test.[jt]s?(x)' --runInBand",
    "test:integration": "node --trace-warnings --unhandled-rejections=strict node_modules/jest/bin/jest.js --testMatch='**/__tests__/integration/**/*.test.[jt]s?(x)'",
    "test:ci": "node --trace-warnings --unhandled-rejections=strict node_modules/jest/bin/jest.js --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "@expo/webpack-config": "19.0.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/app": "18.3.0",
    "@react-native-firebase/firestore": "18.3.0",
    "@react-navigation/native": "6.1.7",
    "@react-navigation/native-stack": "6.9.13",
    "expo": "49.0.8",
    "expo-barcode-scanner": "12.5.3",
    "expo-haptics": "12.4.0",
    "expo-sharing": "11.5.0",
    "expo-status-bar": "1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-qrcode-svg": "6.3.15",
    "react-native-safe-area-context": "4.7.1",
    "react-native-screens": "3.24.0",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "0.19.8"
  },
  "devDependencies": {
    "@babel/core": "7.22.0",
    "@testing-library/react-native": "12.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "18.2.14",
    "@types/react-native": "0.72.2",
    "@types/react-test-renderer": "^19.1.0",
    "jest-expo": "^53.0.9",
    "jest-mock": "29.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.1.3"
  },
  "private": true,
  "homepage": "https://blackursa.github.io/digidex/"
}
